   "source": [
    "n = int(input())\n",
    "\n",
    "def printProcess(i,N,down):\n",
    "    if i > N:\n",
    "        return \n",
    "    printProcess(i+1, N, True)  #先右\n",
    "    print(\"down\" if down else \"up\") #再中\n",
    "    printProcess(i+1, N, False) #最后左\n",
    "\n",
    "def printAllFolds(N):\n",
    "    printProcess(1,N,True)\n",
    "    \n",
    "printAllFolds(n)"
   ]
