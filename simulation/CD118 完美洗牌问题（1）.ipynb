    "n = int(input())\n",
    "arr = input().split()\n",
    "\n",
    "def trans(arr,n):\n",
    "    mid = n // 2\n",
    "    ans = []\n",
    "    for i in range(mid):\n",
    "        ans.append(arr[i+mid])\n",
    "        ans.append(arr[i])\n",
    "    return ans \n",
    "ans = trans(arr,n)\n",
    "print(*ans)"
