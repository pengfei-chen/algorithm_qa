    "chas = input().split()\n",
    "\n",
    "def reverse(chas):\n",
    "    for i in range(len(chas)):\n",
    "        x = chas[i]\n",
    "        chas[i] = x[::-1]\n",
    "    return chas \n",
    "\n",
    "ans = reverse(chas)\n",
    "ans = ' '.join(ans)\n",
    "print(ans)\n"
   ]
