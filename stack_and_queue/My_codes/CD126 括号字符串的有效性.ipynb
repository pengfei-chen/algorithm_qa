   "source": [
    "line = input()\n",
    "\n",
    "cnt = 0\n",
    "for s in line:\n",
    "    if s == '(':\n",
    "        cnt += 1\n",
    "    elif s == ')':\n",
    "        cnt -= 1\n",
    "    else:\n",
    "        cnt += 1\n",
    "    if cnt < 0:\n",
    "        break\n",
    "if cnt == 0:\n",
    "    print('YES')\n",
    "else:\n",
    "    print('NO')"
   ]
