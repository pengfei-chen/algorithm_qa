    "import heapq\n",
    "\n",
    "n,k = list(map(int,input().split()))\n",
    "l = list(map(int,input().split()))\n",
    "\n",
    "heapq.heapify(l)\n",
    "ans = heapq.nsmallest(k,l)  #直接用堆的nsmallest方法\n",
    "print(*ans)"
   ]
