   "source": [
    "def numOfZero(n):\n",
    "    num, i = 0, 5\n",
    "    while i <= n:\n",
    "        num += n // i \n",
    "        i *= 5 \n",
    "    return num\n",
    "n = int(input())\n",
    "print(numOfZero(n))"
   ]
  },
